{
  "name": "@technicalsupport/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.app.json && tsc --noEmit --project ./tsconfig.node.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache src/**/*.less",
    "prettify": "prettier --log-level warn --cache --write \"src/**/*.{ts,tsx,js,json,yml,scss}\" \"*.{ts,tsx,js,json,yml,scss}\" --ignore-path .prettierignore"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@tanstack/react-query": "4.32.6",
    "@technicalsupport/backend": "workspace:*",
    "@trpc/client": "10.37.1",
    "@trpc/react-query": "10.37.1",
    "antd": "^5.24.7",
    "dayjs": "^1.11.13",
    "react": "^19.0.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.1",
    "superjson": "^2.2.2"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "classnames": "^2.5.1",
    "globals": "^15.15.0",
    "less": "^4.3.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.19.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-less": "^3.0.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-less": "^3.0.1",
    "typescript": "5.1.6",
    "vite": "^6.2.0",
    "vite-plugin-svgr": "^4.3.0"
  }
}
